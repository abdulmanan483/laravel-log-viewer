{
  "name": "abdulmanan483/laravel-log-viewer",
  "description": "Provides a Log Viewer for Laravel",
  "keywords": [
    "abdulmananww",
    "abdulmanan483",
    "laravel",
    "log",
    "log viewer",
    "log-viewer",
    "logviewer"
  ],
  "homepage": "https://github.com/abdulmanan483/laravel-log-viewer",
  "authors": [
    {
      "name": "Abdul Manan",
      "email": "abdulmananww@gmail.com",
      "homepage": "https://github.com/abdulmanan483",
      "role": "Developer"
    }
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "abdulmanan483/laravel-support": "1.0",
    "psr/log": "^1.0|^2.0|^3.0"
  },
  "require-dev": {
    "laravel/framework": "^12.0",
    "mockery/mockery": "^1.6",
    "orchestra/testbench-core": "^10.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Arcanedev\\LogViewer\\": "src/"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Arcanedev\\LogViewer\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always",
    "test:dox": "phpunit --testdox --colors=always",
    "test:cov": "phpunit --coverage-html build/coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arcanedev\\LogViewer\\LogViewerServiceProvider",
        "Arcanedev\\LogViewer\\Providers\\DeferredServicesProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
